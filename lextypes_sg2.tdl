basic_bse_cp_verb := basic_cp_prop+ques_verb & verb_synsem &
  [ LOCAL.CAT.VAL.KCMP [ LOCAL.CAT s_cat_bse ] ].

bse_cp_verb := basic_bse_cp_verb &
