                   HCONS <! qeq & [ HARG #rhand,
                                    LARG #nhand ] !>,
		   ICONS <! !> ] ],
    LKEYS.ALTKEYREL #altkeyrel ].

  
; 'inform Kim that S'
basic_cp_trans_subst := basic_three_arg_trans_subst & cp_addin_tam_pn &
