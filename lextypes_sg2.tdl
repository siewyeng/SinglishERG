expl_cp_subst :=  norm_two_arg_subst & cp_addin_tam_pn &
  [ LOCAL.CAT.VAL [ COMPS < synsem & #keycomp &
                            [ LOCAL [ CAT s_cat_fin_v_c & [ HEAD verbal ],
