      LKEYS [ KEYREL #keyrel & [ PRED some_q_rel,
				 ARG0 #index & ref-ind,
				 RSTR #rhand ],
	      ALTKEYREL #altkeyrel ],
      NONLOC [ SLASH 0-dlist,
               QUE 0-dlist,
               REL 0-dlist ] ],
    INFLECTD + ].

