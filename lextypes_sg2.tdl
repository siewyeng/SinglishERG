			 CFROM #from, CTO #to ] !>,
               HCONS <! qeq & [ HARG #rhand,
				LARG #nhand ] !>,
	       ICONS <! !> ],
	AGR [ DIV -,
	      PNG.PN #pn ] ],
      LKEYS [ KEYREL.PRED interval_rel,
	      --+COMPKEY card_rel,
	      --+OCOMPKEY card_rel ] ] ].
