                        [ LOCAL [ CAT [ HEAD verb & 
                                             [ MINORS.ALTMIN non_ellipt_rel,
					       TAM indic_tam ],
                                        VAL [ SUBJ *anti_list*,
					      SPR *olist* ],
                                        MC - ],
                                  CONT.HOOK [ LTOP #ltop,
					      INDEX.SF basic-prop ] ] ] >,
		    KCMP #keycomp ] ],
        CONT.HOOK.INDEX #ltop & handle ],
