						       CONT.HOOK [ INDEX #ind &
								      ref-ind],
						       AGR #slagr ] !> ] >,
		   CONT [ HOOK.XARG #ind,
			  RELS <! [ PRED udef_q_rel,
				    ARG0 #ind,
				    RSTR #rhand,
				    CFROM #cfrom, CTO #cto ],
				  [ LBL #nhand,
				    ARG0 #ind,
				    PRED generic_entity_rel,
				    CFROM #cfrom, CTO #cto ] !>,
			  HCONS <! qeq & [ HARG #rhand,
